{
    "manufacturer": "LUANTY",
    "keyboard_name": "pheromone",
    "maintainer": "LUANTY",
    "url": "https://github.com/luantty2/pheromone_keyboard",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x6060",
        "vid": "0xFEED"
    },
    "bootloader": "qmk-dfu",
    "diode_direction": "COL2ROW",
    "features": {
        "bootmagic": false,
        "extrakey": false,
        "mousekey": true,
        "console": true,
        "command": false,
        "nkro": false,
        "backlight": false,
        "rgblight": false,
        "audio": false,
        "key_lock": false,
        "oled_driver": false,
        "via": false,
        "midi": true,
        "encoder": true
    },
    "encoder": {
        "rotary": [
            {
                "pin_a": "B7",
                "pin_b": "D2",
                "resolution": 4
            }
        ]
    },
    "matrix_pins": {
        "rows": [
            "B3",
            "B2",
            "B1",
            "B0",
            "E6"
        ],
        "cols": [
            "D3",
            "D5",
            "D4",
            "D6",
            "D7",
            "B4",
            "B5",
            "B6",
            "C6",
            "C7",
            "F7",
            "F6",
            "F4",
            "F1",
            "F0"
        ]
    },
    "processor": "atmega32u4",
